            "jsx": true
        },
        "ecmaVersion": 2017
    },
    "settings": {
        "react": {
            "version": "17.0.2"
        }
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "plugins": [
        "react"
    ]
}
